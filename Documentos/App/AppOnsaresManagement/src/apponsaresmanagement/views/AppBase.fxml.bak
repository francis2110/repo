<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?language javascript?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml" fx:controller="apponsaresmanagement.controllers.AppBaseController">
  <children>
    <BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <top>
        <MenuBar>
          <menus>
            <Menu mnemonicParsing="false" text="Transporte">
              <items>
                <Menu mnemonicParsing="false" text="Cliente">
                  <items>
                    <MenuItem id="nuevoCliente" mnemonicParsing="false" onAction="#newClient" text="Nuevo cliente" fx:id="nuevoClienteMIt" />
                    <MenuItem mnemonicParsing="false" onAction="#seeClient" text="Ver cliente" fx:id="seeClientItm" />
                    <MenuItem mnemonicParsing="false" text="Editar cliente" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Viaje">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Nuevo viaje" />
                    <MenuItem mnemonicParsing="false" text="Ver viaje" />
                    <MenuItem mnemonicParsing="false" text="Editar viaje" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Transportistas">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Nuevo transportista" />
                    <MenuItem mnemonicParsing="false" text="Ver transportista" />
                    <MenuItem mnemonicParsing="false" text="Editar transportista" />
                  </items>
                </Menu>
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Remolques">
              <items>
                <MenuItem mnemonicParsing="false" text="Delete" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Balances">
              <items>
                <MenuItem mnemonicParsing="false" text="About" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
      </top>
    </BorderPane>
  </children>
  <stylesheets>
    <URL value="@../../appbase.css" />
  </stylesheets>
</AnchorPane>
