    <BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <stylesheets>
        <URL value="@../../seeclient.css" />
      </stylesheets>
      <top>
        <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER_LEFT">
          <children>
            <Label text="Nombre de cliente:" />
            <TextField fx:id="clientNameTxt" prefWidth="200.0">
              <stylesheets>
                <URL value="@../../seeclient.css" />
              </stylesheets>
            </TextField>
            <Button fx:id="searchClientBtn" mnemonicParsing="false" onAction="#searchClient" text="Buscar" />
            <Text fx:id="validatorTxt" fill="#990000" strokeType="OUTSIDE" strokeWidth="0.0">
              <font>
                <Font name="Arial Black" size="16.0" />
              </font>
            </Text>
          </children>
        </HBox>
      </top>
    </BorderPane>
  </children>
  <stylesheets>
    <URL value="@../../../seeclient.css" />
  </stylesheets>
